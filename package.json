{
  "name": "to-do",
  "version": "1.0.0",
  "description": "This is a to-do list app.",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "test": "jest --watchAll --verbose",
    "dev": "run-p client server",
    "client": "webpack --watch --config client/webpack.config.js",
    "server": "nodemon server",
    "lint": "eslint . --ext .js,.jsx",
    "knex": "knex --knexfile server/db/knexfile.js"
  },
  "author": "Michael Hung",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@mui/utils": "^5.10.9",
    "@mui/x-data-grid": "^5.17.8",
    "@mui/x-date-pickers": "^5.0.6",
    "@vscode/sqlite3": "^5.0.7",
    "dayjs": "^1.11.6",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "knex": "^1.0.1",
    "luxon": "^3.0.4",
    "mui": "^0.0.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.8.0",
    "react-datetime": "^3.2.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "regenerator-runtime": "^0.13.7",
    "superagent": "^6.1.0",
    "use-state-if-mounted": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "babel-loader": "^8.2.2",
    "eslint": "^8.8.0",
    "eslint-config-eda": "^1.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "supertest": "^6.1.3",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
